{
    "name": "gess32/patterns",
    "type": "project",
    "description": "Examples of implementation of design patterns in PHP",
    "keywords": [
        "patterns",
        "php"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "State\\": "State/",
            "ChainOfResponsibility\\": "ChainOfResponsibility/",
            "App\\AbstractFactory\\": "AbstractFactory/",
            "App\\Builder\\": "Builder/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
